<div class="container" style="background-image: url(kitchen.jpg);">
<%= simple_form_for [@kitchen] do |f| %>
  <%= f.input :title, label: "Kitchen's name"%>
  <%= f.input :city, label: "Adresss" %>
  <%= f.input :description, label: "Description" %>
  <%= f.input :price_per_hour, label: "Price(R$/Day)" %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit "Add a Kitchen", class: "btn btn-primary" %>
  <% end %>
</div>
