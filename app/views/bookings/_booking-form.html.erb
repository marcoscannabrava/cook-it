<%= simple_form_for(booking, url: kitchen_bookings_path(kitchen.id, booking)) do |f| %>
  <%= f.input :start_at, as: :string, required: false, input_html: { id: "time_start" } %>
  <%= f.input :end_at, as: :string, required: false, input_html: { id: "time_end" } %>
  <%= f.button :submit %>
<% end %>

  